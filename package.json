  "bugs": {
    "url": "https://github.com/AriAlas/project-3/issues"
  },
  "homepage": "https://github.com/AriAlas/project-3#readme",
  "dependencies": {
    "bcrypt": "^3.0.5",
    "bcrypt-nodejs": "0.0.3",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "if-env": "^1.0.4",
    "jquery": "^3.4.0",
    "jsonwebtoken": "^8.5.1",
    "materialize-css": "^1.0.0",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.5",
    "path": "^0.12.7",
    "react-google-places-autocomplete": "^1.3.1",
    "react-materialize": "^3.1.1",
    "sequelize": "^5.3.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.11"
  }
